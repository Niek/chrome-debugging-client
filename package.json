{
  "name": "chrome-debugging-client",
  "version": "0.6.3",
  "description": "An async/await friendly Chrome debugging client with TypeScript support",
  "keywords": [
    "CDP",
    "chrome",
    "debugging",
    "protocol",
    "typescript"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/lib",
    "dist/protocol",
    "dist/codegen"
  ],
  "directories": {
    "dist": "dist",
    "protocol": "dist/protocol",
    "codegen": "dist/codegen"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run rebuild",
    "compile-protocol-gen": "tsc -p scripts/tsconfig.json",
    "protocol-gen": "npm run compile-protocol-gen && node dist/scripts/generate-protocol.js",
    "rebuild": "rm -rf dist && tsc",
    "compile": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "test": "tsc && node dist/test/index.js | faucet"
  },
  "author": "Kris Selden <kris.selden@gmail.com>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/devtrace/chrome-debugging-client.git"
  },
  "dependencies": {
    "@types/node": "^8.9.5",
    "@types/rimraf": "^2.0.2",
    "@types/tape": "^4.2.29",
    "@types/ws": "^4.0.1",
    "chrome-launcher": "^0.10.2",
    "execa": "^0.10.0",
    "mktemp": "^0.4.0",
    "rimraf": "^2.5.1",
    "ws": "^4.1.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/got": "^7.1.8",
    "faucet": "0.0.1",
    "got": "^8.3.0",
    "tape": "^4.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  },
  "engine": "node >= 4.0.0"
}
